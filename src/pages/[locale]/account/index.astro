---
import BaseLayout from '@/layouts/BaseLayout.astro'
import { getLocaleFromParams, getI18n } from '@/i18n/utils'
export { getLocaleStaticPaths as getStaticPaths } from '@/i18n/astro'
const locale = getLocaleFromParams(Astro.params)
const { t } = getI18n(locale)
import AccountPanel from '@/components/shared/AccountPanel'
---

<BaseLayout title={`Account | ${t('common','siteName')}`}>
  <h1 class="text-2xl font-semibold mb-4">Account</h1>
  <AccountPanel client:load />
  
</BaseLayout>
